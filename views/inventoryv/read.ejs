            <input type="text" class="form-control" value="" v-model="name" />
          </div>
        </div>
        <div class="col-sm-12">
          <a href="#" class="btn btn-success" @click="create">Create Warehouse</a>
          <span v-if="processing">Please Wait ...</span>
          <span v-if="status == 'Created'">{{ $data.status }}</span>
        </div>
      </div> <!-- / row -->

      <div>&nbsp;</div>

      <div class="row" v-if="debug">
        <div class="col-sm-12">
          <pre>{{ $data }}</pre>
