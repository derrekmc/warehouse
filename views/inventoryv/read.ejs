          <h3>
            Add Inventory
          </h3>

          <ul id="example-1">
            <li v-for="item in items">
              {{ item.name }}
            </li>
          </ul>

        </div>
      </div> <!-- / row -->

      <div class="row">
        <div class="col-sm-12">
          <h3>
